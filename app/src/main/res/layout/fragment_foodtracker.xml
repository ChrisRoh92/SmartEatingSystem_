<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundLight"
    android:orientation="vertical" android:id="@+id/fragment_foodtracker_background">

    <androidx.core.widget.NestedScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="2dp"
        android:fillViewport="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView4"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    app:cardCornerRadius="2dp"
                    app:cardElevation="2dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ProgressBar
                            android:id="@+id/background_progress_test2"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="150dp"
                            android:layout_height="150dp"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:max="3600"
                            android:progress="2700"
                            android:progressDrawable="@drawable/circle"
                            android:progressTint="@color/backgroundLight"
                            android:rotation="135"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ProgressBar
                            android:id="@+id/fragment_foodtracker_pb_kcal"
                            style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                            android:layout_width="150dp"

                            android:layout_height="150dp"
                            android:backgroundTint="@color/background"
                            android:max="3600"
                            android:progress="0"
                            android:progressDrawable="@drawable/circle"
                            android:rotation="135"
                            app:layout_constraintBottom_toBottomOf="@+id/background_progress_test2"
                            app:layout_constraintEnd_toEndOf="@+id/background_progress_test2"
                            app:layout_constraintStart_toStartOf="@+id/background_progress_test2"
                            app:layout_constraintTop_toTopOf="@+id/background_progress_test2" />

                        <LinearLayout
                            android:id="@+id/linearLayout"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="16dp"
                            android:layout_marginEnd="8dp"
                            android:orientation="horizontal"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.6"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/fragment_foodtracker_tv_aim_progress">

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ProgressBar
                                        android:id="@+id/fragment_foodtracker_pb_Kohlenhydrate"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="8dp"
                                        android:layout_marginEnd="16dp"
                                        android:max="100"
                                        android:progress="0"
                                        app:layout_constraintBottom_toTopOf="@+id/fragment_foodtracker_tv_carb"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintHorizontal_bias="1.0"
                                        app:layout_constraintStart_toStartOf="parent" />

                                    <TextView
                                        android:id="@+id/textView2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Kohlenhydrate"
                                        android:textColor="@color/textColor1"
                                        app:layout_constraintBottom_toTopOf="@+id/fragment_foodtracker_pb_Kohlenhydrate"
                                        app:layout_constraintEnd_toEndOf="@+id/fragment_foodtracker_pb_Kohlenhydrate"
                                        app:layout_constraintStart_toStartOf="@+id/fragment_foodtracker_pb_Kohlenhydrate" />

                                    <TextView
                                        android:id="@+id/fragment_foodtracker_tv_carb"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="8dp"
                                        android:textSize="10sp"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintEnd_toEndOf="@+id/fragment_foodtracker_pb_Kohlenhydrate"
                                        app:layout_constraintStart_toStartOf="@+id/fragment_foodtracker_pb_Kohlenhydrate" />
                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ProgressBar
                                        android:id="@+id/fragment_foodtracker_pb_protein"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="16dp"
                                        android:layout_marginEnd="16dp"
                                        android:max="100"
                                        android:progress="0"
                                        app:layout_constraintBottom_toTopOf="@+id/fragment_foodtracker_tv_protein"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintHorizontal_bias="1.0"
                                        app:layout_constraintStart_toStartOf="parent" />

                                    <TextView
                                        android:id="@+id/textView2111"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Proteine"
                                        android:textColor="@color/textColor1"
                                        app:layout_constraintBottom_toTopOf="@+id/fragment_foodtracker_pb_protein"
                                        app:layout_constraintEnd_toEndOf="@+id/fragment_foodtracker_pb_protein"
                                        app:layout_constraintStart_toStartOf="@+id/fragment_foodtracker_pb_protein" />

                                    <TextView
                                        android:id="@+id/fragment_foodtracker_tv_protein"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="8dp"
                                        android:textSize="10sp"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintEnd_toEndOf="@+id/fragment_foodtracker_pb_protein"
                                        app:layout_constraintStart_toStartOf="@+id/fragment_foodtracker_pb_protein" />
                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ProgressBar
                                        android:id="@+id/fragment_foodtracker_pb_fett"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="16dp"
                                        android:layout_marginEnd="16dp"
                                        android:max="100"
                                        android:progress="0"
                                        app:layout_constraintBottom_toTopOf="@+id/fragment_foodtracker_tv_fett"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintHorizontal_bias="1.0"
                                        app:layout_constraintStart_toStartOf="parent" />

                                    <TextView
                                        android:id="@+id/textView2222"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Fett"
                                        android:textColor="@color/textColor1"
                                        app:layout_constraintBottom_toTopOf="@id/fragment_foodtracker_pb_fett"
                                        app:layout_constraintEnd_toEndOf="@id/fragment_foodtracker_pb_fett"
                                        app:layout_constraintStart_toStartOf="@id/fragment_foodtracker_pb_fett" />

                                    <TextView
                                        android:id="@+id/fragment_foodtracker_tv_fett"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="8dp"
                                        android:textSize="10sp"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintEnd_toEndOf="@id/fragment_foodtracker_pb_fett"
                                        app:layout_constraintStart_toStartOf="@id/fragment_foodtracker_pb_fett" />
                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </FrameLayout>
                        </LinearLayout>

                        <TextView
                            android:id="@+id/textView19"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginEnd="8dp"
                            android:layout_marginBottom="8dp"
                            android:text="Verfügbar"
                            android:textColor="@color/background"
                            android:textSize="18sp"
                            app:layout_constraintBottom_toBottomOf="@+id/fragment_foodtracker_pb_kcal"
                            app:layout_constraintEnd_toEndOf="@+id/fragment_foodtracker_pb_kcal"
                            app:layout_constraintStart_toStartOf="@+id/fragment_foodtracker_pb_kcal"
                            app:layout_constraintTop_toTopOf="@+id/fragment_foodtracker_pb_kcal"
                            app:layout_constraintVertical_bias="0.45" />

                        <TextView
                            android:id="@+id/fragment_foodtracker_tv_kcal_rest"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="10sp"
                            app:layout_constraintEnd_toEndOf="@+id/textView19"
                            app:layout_constraintStart_toStartOf="@+id/textView19"
                            app:layout_constraintTop_toBottomOf="@+id/textView19" />

                        <TextView
                            android:id="@+id/textView52"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="32dp"
                            android:layout_marginEnd="8dp"
                            android:text="Konsumiert"
                            android:textColor="@color/textColor1"
                            android:textSize="18sp"
                            app:layout_constraintEnd_toStartOf="@+id/fragment_foodtracker_pb_kcal"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/fragment_foodtracker_tv_kcal_added"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="10sp"
                            app:layout_constraintEnd_toEndOf="@+id/textView52"
                            app:layout_constraintStart_toStartOf="@+id/textView52"
                            app:layout_constraintTop_toBottomOf="@+id/textView52" />

                        <TextView
                            android:id="@+id/textView54"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Aktivität"
                            android:textColor="@color/textColor1"
                            android:textSize="18sp"
                            app:layout_constraintBottom_toTopOf="@+id/fragment_foodtracker_tv_kcal_active"
                            app:layout_constraintEnd_toEndOf="@+id/textView52"
                            app:layout_constraintStart_toStartOf="@+id/textView52" />

                        <TextView
                            android:id="@+id/fragment_foodtracker_tv_kcal_active"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="24dp"
                            android:text="0 Kcal"
                            android:textSize="10sp"
                            app:layout_constraintBottom_toBottomOf="@+id/fragment_foodtracker_pb_kcal"
                            app:layout_constraintEnd_toEndOf="@+id/textView54"
                            app:layout_constraintStart_toStartOf="@+id/textView54" />

                        <TextView
                            android:id="@+id/textView56"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginEnd="16dp"
                            android:text="Ziel"
                            android:textColor="@color/textColor1"
                            android:textSize="18sp"
                            app:layout_constraintBottom_toTopOf="@+id/fragment_foodtracker_tv_kcal_added"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/fragment_foodtracker_pb_kcal"
                            app:layout_constraintTop_toTopOf="@+id/textView52" />

                        <TextView
                            android:id="@+id/fragment_foodtracker_tv_aim"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Abnehmen"
                            android:textSize="10sp"
                            app:layout_constraintEnd_toEndOf="@+id/textView56"
                            app:layout_constraintStart_toStartOf="@+id/textView56"
                            app:layout_constraintTop_toBottomOf="@+id/textView56" />

                        <TextView
                            android:id="@+id/textView58"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:layout_marginBottom="8dp"
                            android:text="Forschritt"
                            android:textColor="@color/textColor1"
                            android:textSize="18sp"
                            app:layout_constraintBottom_toTopOf="@+id/fragment_foodtracker_tv_kcal_active"
                            app:layout_constraintEnd_toEndOf="@+id/textView56"
                            app:layout_constraintStart_toStartOf="@+id/textView56"
                            app:layout_constraintTop_toTopOf="@+id/textView54" />

                        <TextView
                            android:id="@+id/fragment_foodtracker_tv_aim_progress"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="6,7 Kg Verloren"
                            android:textSize="10sp"
                            app:layout_constraintEnd_toEndOf="@+id/textView58"
                            app:layout_constraintStart_toStartOf="@+id/textView58"
                            app:layout_constraintTop_toBottomOf="@+id/textView58" />

                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/foodtracker_mealcard_breakfast"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:alpha="1"
                    app:cardCornerRadius="2dp"
                    app:cardElevation="2dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardView2">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/foodtracker_mealcard_breakfast_tv_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:text="Frühstück"
                            android:textColor="@color/textColor1"
                            android:textSize="18sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <View
                            android:id="@+id/divider13"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:background="@color/backgroundLight"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/foodtracker_mealcard_breakfast_tv_title" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/foodtracker_mealcard_breakfast_rv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:minHeight="50dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/divider13"
                            tools:listitem="@layout/recycleritem_mealcard_foodtracker" />

                        <View
                            android:id="@+id/divider7"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:background="@color/backgroundLight"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/foodtracker_mealcard_breakfast_rv" />

                        <Button
                            android:id="@+id/foodtracker_mealcard_breakfast_btn"
                            style="@style/Widget.MaterialComponents.Button.TextButton.Icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:insetTop="10dp"
                            android:insetBottom="10dp"
                            android:text="Neues Lebensmittel"
                            app:icon="@drawable/ic_add_white"
                            app:iconSize="20dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/divider7"
                            app:layout_constraintTop_toBottomOf="@+id/divider7" />

                        <TextView
                            android:id="@+id/foodtracker_mealcard_breakfast_tv_kcal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="8dp"
                            android:alpha="0"
                            android:text="475 Kcal"
                            android:textColor="@color/textColor1"
                            android:textSize="18sp"
                            app:layout_constraintBottom_toBottomOf="@id/foodtracker_mealcard_breakfast_tv_title"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="@id/foodtracker_mealcard_breakfast_tv_title" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/foodtracker_mealcard_lunch"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:alpha="1"
                    app:cardCornerRadius="2dp"
                    app:cardElevation="2dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/foodtracker_mealcard_breakfast">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/foodtracker_mealcard_lunch_tv_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:text="Mittagessen"
                            android:textColor="@color/textColor1"
                            android:textSize="18sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <View
                            android:id="@+id/divider1333"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:background="@color/backgroundLight"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/foodtracker_mealcard_lunch_tv_title" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/foodtracker_mealcard_lunch_rv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:minHeight="50dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/divider1333"
                            tools:listitem="@layout/recycleritem_mealcard_foodtracker" />

                        <View
                            android:id="@+id/divider7313"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:background="@color/backgroundLight"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/foodtracker_mealcard_lunch_rv" />

                        <Button
                            android:id="@+id/foodtracker_mealcard_lunch_btn"
                            style="@style/Widget.MaterialComponents.Button.TextButton.Icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:insetTop="10dp"
                            android:insetBottom="10dp"
                            android:text="Neues Lebensmittel"
                            app:icon="@drawable/ic_add_white"
                            app:iconSize="20dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="@id/divider7313"
                            app:layout_constraintTop_toBottomOf="@id/divider7313" />

                        <TextView
                            android:id="@+id/foodtracker_mealcard_lunch_tv_kcal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="8dp"
                            android:alpha="0"
                            android:text="475 Kcal"
                            android:textColor="@color/textColor1"
                            android:textSize="18sp"
                            app:layout_constraintBottom_toBottomOf="@id/foodtracker_mealcard_lunch_tv_title"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="@id/foodtracker_mealcard_lunch_tv_title" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/foodtracker_mealcard_dinner"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:alpha="1"
                    app:cardCornerRadius="2dp"
                    app:cardElevation="2dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/foodtracker_mealcard_lunch">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/foodtracker_mealcard_dinner_tv_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:text="Abendessen"
                            android:textColor="@color/textColor1"
                            android:textSize="18sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <View
                            android:id="@+id/divider1321"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:background="@color/backgroundLight"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/foodtracker_mealcard_dinner_tv_title" />

                        <androidx.recyclerview.widget.RecyclerView
